    "chalk": "^2.4.2",
    "cld": "^2.9.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.3",
    "express-validator": "^7.0.1",
    "i18next": "^23.6.0",
    "i18next-fs-backend": "^2.2.0",
    "i18next-http-middleware": "^3.4.1",
    "joi": "^17.11.0",
