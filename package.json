{
  "name": "ecommerce-admin-sql",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index",
    "lint": "eslint ./ src --ext .js,.ts",
    "lint:debug": "eslint --debug ./src --ext .js,.ts",
    "lint:fix": "eslint ./ src --ext .js,.ts --fix",
    "test": "jest",
    "prettier": "npx pretty-quick --staged --config ./.prettierrc",
    "create:db": "sequelize --config src/config/admindb.config.js db:create",
    "migrate:up": "sequelize --config src/config/admindb.config.js --migrations-path=src/databases/admin/migrations db:migrate",
    "migrate:down": "sequelize --config src/config/admindb.config.js --migrations-path=src/databases/admin/migrations db:migrate:undo:all",
    "seed": "sequelize --config src/config/admindb.config.js --seeders-path=src/databases/admin/seeders db:seed:all",
    "unseed": "sequelize --config src/config/admindb.config.js --seeders-path=src/databases/admin/seeders db:seed:undo:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
